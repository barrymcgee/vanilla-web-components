---
extends: google
plugins:
  - html
rules:
  default-case: 0
  indent: [2, 2, {"SwitchCase": 1}]
  no-console: 0
  no-inline-comments: 0
  space-before-function-paren: [2, "never"]
  require-jsdoc: 0
globals:
  '__dirname': false
  'app': false
  'page': false
  'Polymer': true
  'process': false
  'require': false
